<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<ScrollPane id="mapScrollPane" fx:id="mapScrollPane" fitToHeight="true" fitToWidth="true" styleClass="backGround" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="map.map.MapController">
   <content>
      <BorderPane styleClass="backGround">
         <center>
            <GridPane id="mapGrid" fx:id="mapGrid" maxHeight="10.0" maxWidth="10.0" styleClass="backGround">
              <columnConstraints>
                <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
              </rowConstraints>
            </GridPane>
         </center>
      </BorderPane>
   </content>
</ScrollPane>
