<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane fx:id="borderPane" styleClass="backGround" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="storesDetails.StoresDetailsController">
   <center>
      <ScrollPane fx:id="scrollPane" fitToHeight="true" fitToWidth="true" prefHeight="400.0" prefWidth="600.0" styleClass="backGround">
         <content>
            <VBox fx:id="vbox" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" styleClass="backGround" />
         </content>
      </ScrollPane>
   </center>
   <top>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Stores:" BorderPane.alignment="CENTER">
         <font>
            <Font size="25.0" />
         </font>
         <BorderPane.margin>
            <Insets bottom="5.0" top="5.0" />
         </BorderPane.margin>
         <styleClass>
            <String fx:value="txt" />
            <String fx:value="title" />
         </styleClass>
      </Text>
   </top>
   <bottom>
      <HBox alignment="CENTER" prefHeight="63.0" prefWidth="600.0" styleClass="backGround" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="cancekBtn" alignment="CENTER" mnemonicParsing="false" onAction="#onClickCancelBtn" prefHeight="50.0" prefWidth="100.0" styleClass="btn" text="cancel" />
         </children>
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
